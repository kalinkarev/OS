#include <stdio.h>
#include <stdlib.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>
#include <unistd.h>
#include <string.h>

#define BUFF_SIZE 420

int main(int argc, char *argv[]) {
	char *filename = argv[1];
	char a = *argv[2], b = *argv[3];
	char buffer[BUFF_SIZE];
	int i, c = 0;
	int fd = open(filename, O_RDONLY);
	
	do {
		c = read(fd, buffer, BUFF_SIZE);
		
		for (i = 0; i < c; i++) {
		
		}
		
		write(STDOUT_FILENO, buffer, c);
		
	} while (c != 0);
	
	close(fd);
	
	return 0;
}
